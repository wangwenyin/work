#containner {
    #content {
        padding-bottom: 60px;
        .trend-chart-box {
            border:1px solid rgba(234,234,234,1);
            .header {
                display: flex;
                justify-content: space-between;
                align-items: center;
                height:42px;
                padding: 0 10px;
                color: #333;
                font-size: 14px;
                background:rgba(248,248,248,1);
                border-bottom:1px solid rgba(234,234,234,1);
                .chart-name {
                    font-weight: bold;
                }
                .date-picker-wrapper {
                    display: flex;
                    .quick-btn {
                        width:80px;
                        height:30px;
                        margin-right: 10px;
                        line-height: 30px;
                        text-align: center;
                        background:rgba(255,255,255,1);
                        border:1px solid rgba(234,234,234,1);
                        cursor: pointer;
                        &.active {
                            color: rgba(140,78,127,1);
                            border:1px solid rgba(140,78,127,1);
                        }
                    }
                    .custom-box {
                        display: flex;
                        align-items: center;
                        .label {
                            margin: 0 10px;
                        }
                        .date-picker {
                            width:278px;
                            height:30px;
                            background:rgba(255,255,255,1);
                            border:1px solid rgba(234,234,234,1);
                        }
                        .search {
                            width:47px;
                            height:30px;
                            background-color:rgba(140,78,127,1);
                            background-image: url(../images/search.png);
                            background-repeat: no-repeat;
                            background-position: center;
                            cursor: pointer;
                        }
                    }
                }
            }
            .chart {
                width: 100%;
                height: 530px;
            }
        }
        .common-table {
            margin-top: 30px;
        }
    }
}