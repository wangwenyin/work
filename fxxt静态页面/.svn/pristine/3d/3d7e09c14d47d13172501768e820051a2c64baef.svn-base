#containner #content {
  padding-bottom: 60px;
}
#containner #content .trend-chart-box {
  border: 1px solid #eaeaea;
}
#containner #content .trend-chart-box .header {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 42px;
  padding: 0 10px;
  color: #333;
  font-size: 14px;
  background: #f8f8f8;
  border-bottom: 1px solid #eaeaea;
}
#containner #content .trend-chart-box .header .chart-name {
  font-weight: bold;
}
#containner #content .trend-chart-box .header .date-picker-wrapper {
  display: flex;
}
#containner #content .trend-chart-box .header .date-picker-wrapper .quick-btn {
  width: 80px;
  height: 30px;
  margin-right: 10px;
  line-height: 30px;
  text-align: center;
  background: #ffffff;
  border: 1px solid #eaeaea;
  cursor: pointer;
}
#containner #content .trend-chart-box .header .date-picker-wrapper .quick-btn.active {
  color: #8c4e7f;
  border: 1px solid #8c4e7f;
}
#containner #content .trend-chart-box .header .date-picker-wrapper .custom-box {
  display: flex;
  align-items: center;
}
#containner #content .trend-chart-box .header .date-picker-wrapper .custom-box .label {
  margin: 0 10px;
}
#containner #content .trend-chart-box .header .date-picker-wrapper .custom-box .date-picker {
  width: 278px;
  height: 30px;
  background: #ffffff;
  border: 1px solid #eaeaea;
}
#containner #content .trend-chart-box .header .date-picker-wrapper .custom-box .search {
  width: 47px;
  height: 30px;
  background-color: #8c4e7f;
  background-image: url(../images/search.png);
  background-repeat: no-repeat;
  background-position: center;
  cursor: pointer;
}
#containner #content .trend-chart-box .chart {
  width: 100%;
  height: 530px;
}
#containner #content .common-table {
  margin-top: 30px;
}
