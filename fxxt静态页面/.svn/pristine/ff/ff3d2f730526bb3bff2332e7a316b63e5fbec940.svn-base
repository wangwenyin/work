#containner #content {
  display: flex;
}
#containner #content .sidebar {
  position: relative;
  width: 160px;
  margin-right: 20px;
  margin-bottom: 77px;
  border: 1px solid #eaeaea;
}
#containner #content .sidebar .title {
  height: 40px;
  line-height: 40px;
  padding-left: 10px;
  color: #333;
  border-bottom: 1px solid #eaeaea;
}
#containner #content .sidebar .toggle-btn {
  position: absolute;
  top: 8px;
  right: 0;
  width: 18px;
  height: 30px;
  background-color: #f4dcef;
  background-image: url(../images/collapse.png);
  background-repeat: no-repeat;
  background-position: center;
  border: 1px solid #8c4e7f;
  border-radius: 4px 0px 0px 4px;
  cursor: pointer;
}
#containner #content .sidebar .ztree {
  overflow: hidden;
}
#containner #content .sidebar .ztree * {
  font-size: 14px;
}
#containner #content .sidebar .ztree li {
  margin-bottom: 10px;
}
#containner #content .sidebar .ztree li a.level0 {
  margin-bottom: 10px;
}
#containner #content .sidebar .ztree li ul.level1 {
  margin-top: 10px;
}
#containner #content .sidebar .ztree li.level1 a {
  color: #666;
}
#containner #content .sidebar .ztree li span.button {
  background-position: center;
}
#containner #content .sidebar .ztree li span.button.roots_open,
#containner #content .sidebar .ztree li span.button.center_open,
#containner #content .sidebar .ztree li span.button.bottom_open {
  background-image: url(../images/tree-open.png);
}
#containner #content .sidebar .ztree li span.button.roots_close,
#containner #content .sidebar .ztree li span.button.center_close,
#containner #content .sidebar .ztree li span.button.bottom_close,
#containner #content .sidebar .ztree li span.button.center_docu,
#containner #content .sidebar .ztree li span.button.bottom_docu {
  background-image: url(../images/tree-close.png);
}
#containner #content .sidebar.collapsed {
  display: none;
}
#containner #content .main-content {
  flex: 1;
}
#containner #content .main-content .filter-box {
  display: flex;
  align-items: center;
}
#containner #content .main-content .filter-box .title {
  position: relative;
  width: 100px;
  height: 40px;
  line-height: 40px;
  text-align: center;
  background: #f8f8f8;
}
#containner #content .main-content .filter-box .title .toggle-btn {
  display: none;
  position: absolute;
  left: 0;
  top: 5px;
  width: 18px;
  height: 30px;
  background-color: #f4dcef;
  background-image: url(../images/expand.png);
  background-repeat: no-repeat;
  background-position: center;
  border: 1px solid #8c4e7f;
  border-radius: 0px 4px 4px 0px;
  cursor: pointer;
}
#containner #content .main-content .filter-box .filter-right {
  flex: 1;
  display: flex;
  align-items: center;
  padding: 0 10px;
  border: 1px solid #eaeaea;
  border-left: none;
}
#containner #content .main-content .filter-box .filter-right .filter-items {
  display: flex;
  flex-wrap: wrap;
}
#containner #content .main-content .filter-box .filter-right .filter-items li {
  height: 30px;
  line-height: 30px;
  margin: 5px 10px;
  margin-right: 0;
  padding-left: 10px;
  padding-right: 26px;
  background-image: url(../images/close.png);
  background-repeat: no-repeat;
  background-position: 92% center;
  background-size: 8px;
  border: 1px dashed #cccccc;
  cursor: pointer;
}
#containner #content .main-content .filter-box .filter-right .filter-items li:hover {
  background-image: url(../images/close-h.png);
  border: 1px dashed #8c4e7f;
}
#containner #content .main-content .filter-box .filter-right .filter-items li:first-child {
  margin-left: 0;
}
#containner #content .main-content .filter-box .filter-right .clear-all {
  min-width: 110px;
  margin-left: 20px;
  padding-left: 20px;
  color: #999;
  background: url(../images/del.png) no-repeat left center;
  cursor: pointer;
}
#containner #content .main-content .search-bar {
  display: flex;
  justify-content: space-between;
  margin-top: 20px;
  padding: 5px 15px;
  background: #f7f7f7;
  border: 1px solid #e9e9e9;
}
#containner #content .main-content .search-bar .left-bar {
  display: flex;
}
#containner #content .main-content .search-bar .left-bar .search {
  display: flex;
  margin-left: 20px;
}
#containner #content .main-content .search-bar .left-bar .search input {
  width: 270px;
  height: 30px;
  line-height: 30px;
  padding-left: 10px;
  font-size: 14px;
  color: #666;
  border: 1px solid #E9E9E9;
}
#containner #content .main-content .search-bar .left-bar .search input::placeholder {
  color: #999;
  font-size: 14px;
}
#containner #content .main-content .search-bar .left-bar .search .search-btn {
  width: 47px;
  height: 30px;
  background: #8B4D7F url(../images/search.png) no-repeat center;
  cursor: pointer;
}
#containner #content .main-content .search-bar .right-bar a {
  display: inline-block;
  width: 74px;
  height: 30px;
  line-height: 30px;
  padding-left: 33px;
  margin-left: 20px;
  color: #fff;
  background-color: #8b4d7f;
  background-repeat: no-repeat;
  border-radius: 2px;
}
#containner #content .main-content .search-bar .right-bar a.view {
  background-image: url(../images/view.png);
  background-position: 14px center;
}
#containner #content .main-content .search-bar .right-bar a.download {
  background-image: url(../images/download.png);
  background-position: 12px center;
}
#containner #content .main-content .statement-box {
  padding: 25px 15px 10px;
  border: 1px solid #e9e9e9;
  border-top: none;
}
#containner #content .main-content .statement-box h2 {
  color: #333;
  font-size: 22px;
  font-weight: bold;
  text-align: center;
}
#containner #content .main-content .statement-box .statement {
  position: relative;
  margin-top: 15px;
}
#containner #content .main-content .statement-box .statement .content {
  color: #656565;
  line-height: 22px;
}
#containner #content .main-content .statement-box .statement .content.overflow {
  height: 70px;
  overflow: hidden;
}
#containner #content .main-content .statement-box .statement .content.auto {
  height: auto;
}
#containner #content .main-content .statement-box .statement .toggle {
  display: none;
  position: absolute;
  right: 0;
  bottom: 3px;
  width: 100px;
  height: 20px;
  line-height: 20px;
  text-align: right;
  color: #8B4D7F;
  background: rgba(255, 255, 255, 0.9);
  cursor: pointer;
}
#containner #content .main-content .statement-box .statement .toggle::after {
  content: '展开全部';
}
#containner #content .main-content .statement-box .statement .toggle.collapsed::after {
  content: '收起';
}
#containner #content .main-content .table-box {
  margin-top: 20px;
}
